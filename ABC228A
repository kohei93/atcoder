#include <iostream>
#include <cmath>
#include<bitset>
#include<vector>
#include<algorithm>
#include <string>


using namespace std;
typedef long long ll;
#define INF 1010000000
#define N20 1048576

int main(void){
    ll s,t,x;
    cin>>s>>t>>x;
    if(s<=t){
        if(s<=x && t>x){
            cout<<"Yes"<<endl;
        }else{
            cout<<"No"<<endl;
        }
    }else{
        if(s<=x || t>x){
            cout<<"Yes"<<endl;
        }else{
            cout<<"No"<<endl;
        }

    }



    return 0;
}
